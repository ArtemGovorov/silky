{
  "name": "@silky/group",
  "version": "0.0.1",
  "description": "Silky Group",
  "sideEffects": false,
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "unpkg": "umd/silky-group.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "coverage": "codecov",
    "dev": "cross-env NODE_ENV=development rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "engines": {
    "node": ">=8"
  },
  "files": [
    "cjs/*",
    "esm/*",
    "umd/*",
    "src"
  ],
  "keywords": [
    "d3",
    "react",
    "charts",
    "data",
    "visualization",
    "data-visualization",
    "svg"
  ],
  "author": "Juan D. Gomez <dg.dave@gmail.com> (https://davidgomez.dev/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.3.0",
    "codecov": "^3.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.5.0-alpha.0",
    "jest": "^24.3.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-testing-library": "^6.0.0",
    "rollup": "^1.5.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.4"
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davegomez/silky"
  },
  "bugs": {
    "url": "https://github.com/davegomez/silky/issues"
  },
  "homepage": "https://github.com/davegomez/silky#readme",
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
